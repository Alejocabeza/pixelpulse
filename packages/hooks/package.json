{
  "name": "pixelui-hooks",
  "version": "1.0.0",
  "description": "This is a hooks component for pixelui",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alejocabeza/pixelui",
    "directory": "packages/hooks/hooks"
  },
  "bugs": {
    "url": "https://github.com/Alejocabeza/pixelui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "dev": "pnpm build:fast --watch",
    "typechecked": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "keywords": [
    "pixelui",
    "hooks",
    "react",
    "useDomRef"
  ],
  "author": {
    "name": "Alejocabeza",
    "url": "https://github.com/Alejocabeza",
    "email": "alejandrocabeza400@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "pixelui-system": "workspace:*",
    "pixelui-avatar": "workspace:*",
    "pixelui-button": "workspace:*",
    "pixelui-checkbox": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "clean-package": "^2.2.0"
  },
  "eslintConfig": {
    "extends": ["standard"],
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "clean-package": "../../../clean-package.config.json"
}
