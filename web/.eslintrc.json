{
  "extends": [
    "../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "parserOptions": {
        "project": [
          "web/tsconfig(.*)?.json"
        ],
        "ecmaFeatures": {
          "jsx": true
        }
      }
    }
  ]
}
